					<Add after="chmod u=rwx ./build/linux/post-build.bash" />
					<Add after="./build/linux/post-build.bash devel $(OFLP_WXVERSION) $(OFLP_WXTOOLKIT)" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
